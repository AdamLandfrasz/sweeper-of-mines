extends layout

block content
  div(id='buttons')
    button(id='beginner')='beginner'
    button(id='intermediate')='intermediate'
    button(id='expert')='expert'
  div(id='wrapper')
    div(id='header')
      div(id='flag-counter' class='digital')='0'
      img(id='reset' src='/images/faces/face_unpressed.svg' alt='reset-game')
      div(id='timer' class='digital')='0'
    div(id='game-field')
